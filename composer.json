{
    "name": "toyrone/atlas-sms-phpwrapper",
    "type": "library",
    "description": "Atlas sms api wrapper",
    "keywords": [
        "atlas-sms",
        ":package_name"
    ],
    "homepage": "https://github.com/toyrone/atlas-sms-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "toyrone",
            "email": "akinstoyosi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "guzzle/guzzle": "~3.9",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            ":vendor\\:package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
